{
    "name": "yuges-code/laravel-groupable",
    "description": "🗃️ Package for easily attaching groups to Laravel eloquent models",
    "keywords": [
        "php",
        "yuges",
        "groups",
        "laravel",
        "package"
    ],
    "homepage": "https://yuges-code.github.io/laravel-groupable",
    "support": {
        "source": "https://github.com/yuges-code/laravel-groupable",
        "issues": "https://github.com/yuges-code/laravel-groupable/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Georgy",
            "email": "goshasafonov@yandex.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "yuges-code/laravel-package": "^1.0",
        "yuges-code/laravel-sluggable": "^1.0",
        "yuges-code/laravel-orderable": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Yuges\\Groupable\\": "src",
            "Yuges\\Groupable\\Database\\Seeders\\": "database/seeders/",
            "Yuges\\Groupable\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yuges\\Groupable\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuges\\Groupable\\Providers\\GroupableServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
